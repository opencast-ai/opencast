{
  "name": "molt-market",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "pnpm -r dev",
    "start": "pnpm --filter @molt/api db:generate && pnpm --filter @molt/api db:deploy && pnpm --filter @molt/api start",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "db:generate": "pnpm --filter @molt/api db:generate",
    "db:migrate": "pnpm --filter @molt/api db:migrate",
    "db:seed": "pnpm --filter @molt/api db:seed",
    "db:deploy": "pnpm --filter @molt/api db:deploy"
  }
}
